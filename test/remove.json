{
  "testCases": [
    {
      "description": "Remove the root from a small non-empty heap",
      "input": [1, 3, 5, 7, 9, 11],
      "expected": [3, 7, 5, 11, 9]
    },
    {
      "description": "Remove the root from a heap with duplicates",
      "input": [2, 3, 3, 5, 5, 7, 9],
      "expected": [3, 5, 3, 9, 5, 7]
    },
    {
      "description": "Remove from a heap with a single element",
      "input": [42],
      "expected": []
    },
    {
      "description": "Remove from an empty heap",
      "input": [],
      "expected": []
    },
    {
      "description": "Remove the root from a larger heap",
      "input": [1, 2, 6, 3, 7, 8, 10, 4, 9],
      "expected": [2, 3, 6, 4, 7, 8, 10, 9]
    }
  ]
}
